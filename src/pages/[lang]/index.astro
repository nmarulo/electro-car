---
import Layout from '../../layouts/Layout.astro';
import Home from '../../components/Home.astro';
import {languages} from '../../i18n/ui';

export function getStaticPaths() {
  return Object.keys(languages)
               .map(lang => ({
                 params: {
                   lang
                 }
               }));
}
